<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>AspNetCore</Authors>
    <Version>2.1.505</Version>
    <Description>An ASP.NET Core authentication middleware: QQConnect for https://connect.qq.com. (腾讯QQ互联/QQ登录）</Description>
    <PackageProjectUrl>https://github.com/myvas/AspNetCore.Authentication.QQConnect</PackageProjectUrl>
    <PackageTags>Myvas,AspNetCore, Authentication, QQ Connect, Tencent</PackageTags>
    <AssemblyName>Myvas.AspNetCore.Authentication.QQConnect</AssemblyName>
    <RootNamespace>Myvas.AspNetCore.Authentication.QQConnect</RootNamespace>
    <PackageId>AspNetCore.Authentication.QQConnect</PackageId>
    <Product>AspNetCore.Authentication</Product>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.OAuth" Version="2.1.2" />
  </ItemGroup>

</Project>
